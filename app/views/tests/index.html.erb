<p id="notice"><%= notice %></p>

<div class="container-fluid">
<h1>Tests</h1>

  <table class="table table-sm table-striped">
    <thead>
      <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Position</th>
        <th>Office</th>
        <th>Age</th>
        <th>Start date</th>
        <th>Salary</th>
      </tr>
    </thead>

    <tbody>
      <% @tests.each do |test| %>
        <tr>
          <td><%= test.id %></td>
          <td><%= test.name %></td>
          <td><%= test.position %></td>
          <td><%= test.office %></td>
          <td><%= test.age %></td>
          <td><%= test.start_date %></td>
          <td><%= test.salary %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
